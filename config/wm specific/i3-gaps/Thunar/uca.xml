<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1661107571133587-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>cm_extractfiles</icon>
	<name>extract (tar.xz)</name>
	<unique-id>1661266039245756-1</unique-id>
	<command>xz -d %n; txz=%n; tar xvf ${txz:0:-3}</command>
	<description></description>
	<patterns>*</patterns>
</action>
<action>
	<icon>object-locked</icon>
	<name>Open Folder as root</name>
	<unique-id>1661269947121364-1</unique-id>
	<command>gksudo thunar %f</command>
	<description>Open a folder as root</description>
	<patterns>*</patterns>
</action>
<action>
	<icon></icon>
	<name>copy file</name>
	<unique-id>1662665094049125-1</unique-id>
	<command>xclip -selection clipboard -t &quot;$(file -b --mime-type %f)&quot; &lt; %f</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
